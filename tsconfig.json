{
  "atom": {
    "rewriteTsconfig": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "noImplicitAny": true,
    "jsx": "react"
  },
  "filesGlob": [
    "**/*.ts",
    "**/*.tsx",
    "!node_modules/**"
  ],
  "files": [
    "typings/globals/material-ui/index.d.ts",
    "typings/globals/react-dom/index.d.ts",
    "typings/globals/react-router/history/index.d.ts",
    "typings/globals/react-router/index.d.ts",
    "typings/globals/react-tap-event-plugin/index.d.ts",
    "typings/globals/react/index.d.ts",
    "typings/index.d.ts",
    "components/Admin/Admin.tsx",
    "components/App.tsx",
    "components/Client/Client.tsx",
    "components/Instructor/Instructor.tsx",
    "components/Login/Login.tsx",
    "components/Routes/Routes.tsx"
  ]
}
